<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="app.client.dominio">

	<joined-subclass name="ModeloSurtidor" extends="Modelo"
		table="modelos_surtidores">

		<key column="modelo"></key>
		<property name="cantidadDePicos" column="cantidadDePicos" />
		<property name="cantidadDeProductos" column="cantidad_productos" />

	</joined-subclass>

</hibernate-mapping>
